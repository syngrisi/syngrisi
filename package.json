{
  "name": "syngrisi-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "publish": "lerna publish",
    "test": "lerna run test --scope=create-sy --scope=@syngrisi/syngrisi",
    "test:unit": "lerna run test --scope=create-sy",
    "test:e2e": "yarn --cwd packages/syngrisi/e2e bddgen && yarn --cwd packages/syngrisi/e2e playwright test --project=chromium --grep-invert '@saml|@sso-external|@sso-logto|@flaky'",
    "test:smoke": "yarn --cwd packages/syngrisi smoke",
    "start": "lerna run start --scope=@syngrisi/syngrisi",
    "build": "lerna run build",
    "graph": "nx graph",
    "docs": "typedoc",
    "e2e:bddgen": "yarn --cwd packages/syngrisi/e2e bddgen",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "yarn build && changeset publish"
  },
  "dependencies": {
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "lerna": "^9.0.3",
    "playwright-bdd": "^8.4.2",
    "vite": "^7.2.6"
  },
  "resolutions": {
    "ws": "^8.18.0",
    "tar-fs": "^3.1.1",
    "bson": "7.0.0",
    "@wdio/logger/chalk": "^4.1.2"
  },
  "overrides": {
    "@wdio/logger": {
      "chalk": "^4.1.2"
    },
    "socks": "^2.8.4",
    "vite": "^7.2.6",
    "bson": "7.0.0",
    "minimatch@3": {
      "brace-expansion": "1.1.12"
    },
    "brace-expansion@1": "1.1.12",
    "minimatch@9.0.4": {
      "brace-expansion": "2.0.2"
    },
    "minimatch@9.0.5": {
      "brace-expansion": "2.0.2"
    }
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  },
  "version": "0.0.0"
}
