{
  "name": "wdio-syngrisi-cucumber-service",
  "version": "2.5.0",
  "description": "WDIO Service for Syngrisi visual tests using Cucumber",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.19.0"
  },
  "np": {
    "releaseDraft": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:syngrisi/syngrisi.git"
  },
  "bugs": {
    "url": "https://github.com/syngrisi/syngrisi/issues"
  },
  "scripts": {
    "pub": "run-s install:global clean compile; np",
    "build": "run-s clean compile",
    "clean": "rimraf ./dist",
    "compile": "babel src/ -d dist/ --config-file ./babel.config.js",
    "test": "vitest run",
    "install:local": "yarn remove @syngrisi/syngrisi-wdio-sdk && yarn add ../wdio-sdk",
    "install:global": "yarn remove @syngrisi/syngrisi-wdio-sdk && yarn add @syngrisi/syngrisi-wdio-sdk",
    "pkglist": "npx npm-packlist # list of files that will be published in syngrisi package, show the files section"
  },
  "keywords": [
    "wdio-plugin",
    "wdio-service",
    "syngrisi"
  ],
  "author": "Viktar Silakou",
  "license": "MIT",
  "devDependencies": {
    "vitest": "^4.0.15",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-function-bind": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-optional-catch-binding": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/register": "^7.28.3",
    "babel-plugin-source-map-support": "^2.2.0",
    "eslint": "^9.39.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@syngrisi/wdio-sdk": "^2.5.0",
    "@wdio/logger": "^9.18.0",
    "rimraf": "^6.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "gitHead": "12bfda406cbe5aaccf3f17fdab02a9bd1a9d6343"
}
